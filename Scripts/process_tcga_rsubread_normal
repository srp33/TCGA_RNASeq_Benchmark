for f in $(pwd)/DownloadSamplesNormal/*
do
  sampleID=$(basename $f)
  sampleID=${sampleID/\.xml/}

  if [ -f FeatureCountsNormal/$sampleID ]
  then
    continue
  fi

  if [ -f InProgressNormal/$sampleID ]
  then
    continue
  fi

  $(pwd)/Scripts/normalize_tcga_rsubread_normal $f temp_parallel_normal
done
